<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/templateListener.xsd">
	<import>
		<templatelistener name="uzbotNotifyNews">
			<environment>admin</environment>
			<templatename>uzbotAdd</templatename>
			<eventname>UzbotNotify</eventname>
			<templatecode><![CDATA[{include file='__uzbotAddNotifyNews' application='news'}]]></templatecode>
		</templatelistener>
		<templatelistener name="uzbotNotifyNewsJS">
			<environment>admin</environment>
			<templatename>uzbotAdd</templatename>
			<eventname>UzbotNotifyJS</eventname>
			<templatecode><![CDATA[{include file='__uzbotAddNotifyNewsJS' application='news'}]]></templatecode>
		</templatelistener>
	</import>
</data>
